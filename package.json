{
  "name": "synk",
  "version": "1.0.0",
  "description": "A simple automation tool that syncs tasks from Notion to Google Calendar",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-webhook": "node synk-fixed/deploy-package/render-backend-server.js",
    "build-webhook": "npm install --prefix synk-fixed/deploy-package",
    "postinstall": "npm install --prefix synk-fixed/deploy-package",
    "dev": "nodemon index.js"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "@supabase/supabase-js": "^2.38.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.1",
    "stripe": "^14.21.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "notion",
    "google-calendar",
    "automation",
    "sync"
  ],
  "author": "",
  "license": "MIT"
}
